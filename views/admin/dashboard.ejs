<%- include ('../partials/header'); %>

<%- include ('../partials/navbar'); %>

<div class="container">
    <div class="row">
        <!--      enctype="multipart/form-data"-->
        <form
                class="col s12 m12 l6 offset-l3"
                action="/dashboard"
                method="post"
                data-app-component="upload"
        >
            <div class="row">
                <div class="input-field">
                    <input
                            id="club"
                            type="text"
                            name="club"
                            class="validate"
                    >
                    <label for="club">Club Name</label>
                </div>
            </div>
            <div class="row">
                <div class="input-field">
                    <input
                            id="country"
                            type="text"
                            name="country"
                            class="validate"
                    >
                    <label for="country">Country</label>
                </div>
            </div>
            <div class="row">
                <div class="file-field input-field">
                    <div class="btn">
                        <span>File</span>
                        <input
                                type="file"
                                name="upload"
                                id="upload-input"
                                data-upload-ref="upload-input"
                        >
                    </div>
                    <div class="file-path-wrapper">
                        <input
                                class="file-path validate"
                                type="text"
                                data-upload-ref="file-path"
                        >
                    </div>
                </div>
            </div>
            <div class="row">
                <button
                        class="waves-effect waves-light btn left"
                        type="button"
                        data-upload-ref="upload-btn"
                >
                    Upload File
                </button>

                <button
                        class="waves-effect waves-light btn right"
                        type="submit"
                >
                    Add Group
                </button>
            </div>
        </form>
    </div>
</div>

