{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 5 versions"
        ]
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-regenerator",
      {
        "asyncGenerators": false,
        "generators": false,
        "async": false
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ]
}
